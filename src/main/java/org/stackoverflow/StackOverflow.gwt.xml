<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="StackOverflow">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.user.theme.clean.Clean' />
    <inherits name='com.sencha.gxt.ui.GXT' />
    <entry-point class='org.stackoverflow.client.StackOverflow'/>
    <inherits name="com.sencha.gxt.chart.Chart"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <source path="client" />
    <source path="shared" />
    <replace-with class='com.sencha.gxt.chart.client.draw.engine.SVG'>
        <when-type-is class='com.sencha.gxt.chart.client.draw.Surface' />
    </replace-with>

    <replace-with class='com.sencha.gxt.chart.client.draw.engine.VML'>
        <when-type-is class='com.sencha.gxt.chart.client.draw.Surface' />
        <any>
            <when-property-is name='user.agent' value='ie6' />
            <when-property-is name='user.agent' value='ie8' />
        </any>
    </replace-with>
    <replace-with class="org.stackoverflow.client.ClientFactoryImpl">
        <when-type-is class="org.stackoverflow.client.ClientFactory"/>
    </replace-with>
</module>